<%if(success && success.length) { %>
  
  <div class="alert alert-success alert-dismissible fade show mt-3 w-90" align="center" role="alert">

    <span ><%= success %></span>
    <button type="button" class="close" data-mdb-dismiss="alert" aria-label="Close" style="float:right">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
  <%} %> <%if(error && error.length) {%>

  <div class="alert alert-danger alert-dismissible fade show" role="alert">
    <%= error %>
    <button type="button" class="close" data-mdb-dismiss="alert" aria-label="Close" style="float:right">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
 
  <%}%>
